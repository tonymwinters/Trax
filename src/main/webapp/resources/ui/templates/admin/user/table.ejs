<thead>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
</thead>
<tbody>
    <% for(var i = 0; i < users.length; ++i){ %>
    <tr id="<%= users[i].id %>">
        <td><%= users[i].firstName %></td>
        <td><%= users[i].lastName %></td>
        <td><%= users[i].contact ? users[i].contact.emailAddress : "" %></td>
    </tr>
    <% } %>
</tbody>