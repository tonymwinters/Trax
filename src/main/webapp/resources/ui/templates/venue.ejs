<thead>
    <th>Name</th>
    <th>Location</th>
    <th>Phone</th>
    <th>Actions</th>
</thead>
<tbody>
    <% for(var i = 0; i < object.length; ++i){ %>
    <tr>
        <td><%= object[i].name %></td>
        <td><%= object[i].location.addressLine1 %></td>
        <td><%= object[i].contact.phoneNumber %></td>
        <td>
            <div class="edit_button"><a href="#">Edit</a></div>
            <div class="delete_button"><a href="#">Delete</a></div>
        </td>
    </tr>
    <% } %>
</tbody>